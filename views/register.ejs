<%- include layouts/header.ejs %>
    
    <div class="page-data">

        <form method="post" action="/user">
            <table cellpadding="11">
            <tr>
                <td class="label">Username</td><td>: <input type="text" name="username" required></td>
            </tr>
            <tr>
                <td class="label">Password</td><td>: <input type="password" name="password" required></td>
            </tr>
            <tr>
                <td class="label">Email</td><td>: <input type="text" name="email" required></td>
            </tr>
            <tr>
                <td class="label"></td>
                <td>
                    &nbsp; <input type="submit" name="Register" value="Register">
                    <input type="button" name="Cancel" value="Cancel" class="cancel" onClick="cancelClick()">
                </td>
            </tr>
            </table>
        </form>
    </div>
<%- include layouts/footer.ejs %>
